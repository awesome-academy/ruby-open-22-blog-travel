<div class="fix_body">
  <div class="fix_center">
    <h1><%= t ".infor" %></h1>
  </div>
  <div class="row">
    <div class="col-3"></div>
    <div class="col-6">
      <%= form_for @district do |f| %>
        <%= render "shared/error_messages", object: f.object %>
        <div class="form-group">
          <h4><%= f.label :name, t(".enter") %></h4>
          <%= f.text_field :name, class:"form-control", placeholder: t(".name") %>
          <div class="fix_center">
            <%= f.submit t(".submit"), class:"form-btn" %>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
